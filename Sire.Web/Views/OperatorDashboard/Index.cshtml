                                    </tr>
                                </thead>
                                <tbody>
                                    @if (Model != null && Model.ToList().Count > 0)
                                    {
                                        foreach (var item in Model)
                                        {
                                            <tr>
                                                <td>
                                                    @item.Vessel.Fleet_id
                                                </td>
                                                <td>
                                                    @item.Vessel.Name
                                                </td>
                                                <td>
                                                    <span class="badge bg-primary rounded-pill">@item.Vessel.Vessel_Type</span>
                                                </td>
                                                <td>
                                                    <a class="btn btn-success" asp-action="" asp-route-Id="@item.Id" style="padding:4px;font-size:9px;" title="Manage"><i class="fa-solid fa-screwdriver-wrench"></i></a>

                                                </td>
                                            </tr>
                                            i = i + 1;
                                        }
