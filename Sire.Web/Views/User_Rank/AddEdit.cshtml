@model Sire.Data.Dto.Master.User_RankDto
@{
    ViewBag.Title = "UserRank";
    Layout = "~/Views/Shared/_LayoutWeb.cshtml";
}

<div class="row fix-head">
    <div class="col-md-12">
        <div class="card m-b-0">
            <div class="card-body head-tit">
                <div class="col-sm-5">
                    <h5 class="head-tit-fnt">User Rank</h5>
                </div>
                <div class="col-sm-7 pt-2">
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End Navigation Fixed Bar-->


<div class="tab-pane fade show active" id="User" role="tabpanel" aria-labelledby="pills-home-tab">
    <!-- End Navigation Fixed Bar-->
    <div class="col-sm-12">
        <div class="card">
            <div class="card-body">
                <br />
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            @Html.Raw(@ViewBag.Alert)
                        </div>
                    </div>
                </div>

                <form asp-action="AddEdit">
                    <hr>
                    @if (@ViewBag.IsEdit)
                    {
                        <input type="hidden" asp-for="Id" />
                    }

                    <div class="col-sm-4 pr-0 h-40">
                        <label class="fnt-lable" for="Rank">Rank Name</label>
                    </div>
                    <div class="col-sm-6 pr-4 h-40">
                        <input type="text" asp-for="Rank" class="form-control inp" />
                        <span asp-validation-for="Rank" class="text-danger"></span>
                    </div>

                    <div class="col-sm-7 pt-2">

                        <button type="submit" class="n-btn  btn-primary" id="btnSubmitOpt">Submit</button>
                        <a class="n-btn btn-danger" asp-action="Index">Back</a>
                        <input type="reset" name="Clear" value="Clear" class="n-btn btn-warning" />
                    </div>
                </form>
            </div>

        </div>
    </div>

</div>
<script src="~/js/pagescript/common.js"></script>


