@model IEnumerable< Sire.Data.Dto.Question.QuestionDto>
@{
    int i = 1;
}
<div class="card" style="margin-bottom:5px">
    <div class="card-body" style="padding:3px">
        <div class="row">
            <div class="col-md-12">
                <button type="button" class="btn btn-success pull-right" style="min-width:100px;">Complete</button>
            </div>
        </div>
    </div>
</div>
@foreach (var item in Model)
{
    <div class="card" style="margin-bottom:5px">
        <div class="card-body">
            <div class="row">
                <a asp-action="AddEdit" asp-controller="InspectionQuestion" asp-route-Id="@item.Id"> @item.Chapter . @item.Section . @i. @item.Questions</a>


            </div>
            <div class="row">
                <div class="col-md-3">
                    <i class="fa fa-eye" aria-hidden="true"></i> <b>2</b>
                </div>
                <div class="col-md-3">
                    <i class="fa fa-camera" aria-hidden="true"></i>
                    <b>3</b>
                </div>
                <div class="col-md-3">
                    <i class="fa fa-microphone" aria-hidden="true"></i>  <b>0</b>
                </div>
                <div class="col-md-3">
                    <i class="fa fa-times" aria-hidden="true"></i>
                    <b>0</b>
                </div>
            </div>
        </div>
    </div>

    i++;
}