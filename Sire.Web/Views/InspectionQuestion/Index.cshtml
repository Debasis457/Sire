@model InspectionQuestionSectionModel

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutWeb.cshtml";
    //int n = 1;

    //var totalCount = Model.QuetionSectionDtos.Select(x => x.Total).Sum(x => x);
    //var assesmentCompletionTotalCount = Model.QuetionSectionDtos.Select(x => x.ResTotal).Sum(x => x);

    //var disableCompletion = assesmentCompletionTotalCount == totalCount ? "" : "disabled";
}

<script src="~/assets/lib/jquery/jquery-3.3.1.min.js"></script>

<script src="~/js/pagescript/Inspection.js"></script>

<form asp-action="Index" asp-controller="InspectionQuestion">

    <div class="row fix-head">
        <div class="col-md-12">

            <div class="card m-b-0">
                <div class="card-body head-tit">
                    <div class="col-sm-7">
                        <h5 class="head-tit-fnt">Inspection Question</h5>
                    </div>
                    <div class="col-sm-1 pr-4 h-40">
                        <input type="hidden" id="hdnInspectionId" value="@ViewBag.InspectionId" />
                    </div>
                    
                    <div class="input-wrapper">
                        
                    </div>

                </div>
            </div>
        </div>
    </div>
</form>
<br />
<br />


<div class="col-md-12 mt-2">
    <div class="card m-b-0">
        <br />
        <br />
        <div class="card-header sub-head-tit">
            <ul class="nav nav-pills nav-fill mt-3 border-bottom pb-3" id="pills-tab" role="tablist">
                <li class="nav-item">
                    <a class="nav-link active" id="pills-home-tab" data-toggle="pill" href="#QueLibrary" role="tab" aria-controls="pills-home" aria-selected="true" onfocus="GetQuestion(@ViewBag.InspectionId)">Question Library</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="pills-profile-tab" data-toggle="pill" href="#ApplicableQue" role="tab" aria-controls="pills-profile" aria-selected="false" onclick="GetApplicableQuestions(@ViewBag.InspectionId)">Applicable Questions</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="pills-contact-tab" data-toggle="pill" href="#RankBaseQue" role="tab" aria-controls="pills-contact" aria-selected="false" onclick="GetRankBasedQuestion(@ViewBag.InspectionId)">Rank Base Questions</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="pills-contact-tab" data-toggle="pill" href="#CIVQ" role="tab" aria-controls="pills-contact" aria-selected="false" onclick="GetCIVQuestion(@ViewBag.InspectionId)">Predicated CIVQ</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="pills-contact-tab" data-toggle="pill" href="#TagQuestion" role="tab" aria-controls="pills-contact" aria-selected="false" onclick="GetTagQuestion(@ViewBag.InspectionId)">Tagged Questions</a>
                </li>

            </ul>
        </div>
        <div class="card-body p-0  pb-5" id="card5_b">
            <div class="tab-content" id="pills-tabContent">
                <div class="tab-pane fade show active" id="QueLibrary" role="tabpanel" aria-labelledby="pills-home-tab">
                </div>
                <div class="tab-pane fade" id="ApplicableQue" role="tabpanel" aria-labelledby="pills-home-tab">
                </div>
                <div class="tab-pane fade" id="RankBaseQue" role="tabpanel" aria-labelledby="pills-home-tab">
                </div>
                <div class="tab-pane fade" id="CIVQ" role="tabpanel" aria-labelledby="pills-home-tab">
                </div>
                <div class="tab-pane fade" id="TagQuestion" role="tabpanel" aria-labelledby="pills-home-tab">
                </div>
            </div>
        </div>
    </div>
</div>
